generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  username String @unique
  password String

  @@map("users")
}

model Link {
  id              String   @id @default(auto()) @map("_id") @db.ObjectId
  originalUrl     String   @unique
  alternativeUrls String[]
  weights         Int[]    @default([])
  accessCount     Int      @default(0)

  @@map("links")
}
